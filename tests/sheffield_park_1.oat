DATASET_URL=https://github.com/pierotofy/drone_dataset_sheffield_park_1/archive/master.zip

@test "with geometric consistency" {
  $run_test "--pc-geometric"
  [ -e "$output_dir/odm_orthophoto/odm_orthophoto.tif" ]
